{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build"
      }
    },
    {
      "src": "api/news.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/news",
      "dest": "api/news.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}